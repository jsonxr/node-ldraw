{
  "name": "ldraw",
  "version": "0.1.1",
  "description": "A Library for parsing an LDraw file",
  "main": "src/ldraw.js",
  "type": "module",
  "scripts": {
    "build": "./node_modules/.bin/tsc",
    "docs": "docsify serve docs",
    "ldraw:install": "curl -o ldraw.zip http://www.ldraw.org/library/updates/complete.zip && unzip ldraw.zip -d examples && rm ldraw.zip",
    "ldraw:generate": "node dist/cli/generate-p.js > src/generated/p.ts",
    "http": "npx http-server .",
    "test": "node_modules/.bin/mocha tests",
    "start": "node dist/server.js"
  },
  "bin": {
    "ldrawclean": "./bin/ldrawclean"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jsonxr/node-ldraw.git"
  },
  "author": "Jason Rowland",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jsonxr/node-ldraw/issues"
  },
  "devDependencies": {
    "@types/cors": "^2.8.8",
    "@types/express": "^4.17.8",
    "@types/morgan": "^1.9.1",
    "@types/node": "^14.11.8",
    "cors": "^2.8.5",
    "docsify-cli": "^4.4.1",
    "express": "^4.17.1",
    "http-proxy-middleware": "^1.0.5",
    "mocha": "^8.1.3",
    "morgan": "^1.10.0",
    "node-localstorage": "^2.1.6",
    "typescript": "^4.0.3"
  },
  "homepage": "https://github.com/jsonxr/node-ldraw",
  "dependencies": {}
}
